# ============================================
# COEXAI - Environment Variables
# Copy this to .env and fill in your values
# ============================================

# App
APP_URL=http://localhost:3000
NODE_ENV=development
PORT=3000

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# OpenAI (for AI agents)
OPENAI_API_KEY=sk-your-key

# Stripe (for payments)
STRIPE_SECRET_KEY=sk_test_your-key
STRIPE_PUBLISHABLE_KEY=pk_test_your-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Stripe Price IDs (create these in Stripe dashboard)
STRIPE_PRICE_STARTER=price_your-starter-price-id
STRIPE_PRICE_GROWTH=price_your-growth-price-id
STRIPE_PRICE_PRO=price_your-pro-price-id
STRIPE_PRICE_EMPIRE=price_your-empire-price-id

# YouTube OAuth
YOUTUBE_CLIENT_ID=your-youtube-client-id
YOUTUBE_CLIENT_SECRET=your-youtube-client-secret

# TikTok OAuth
TIKTOK_CLIENT_KEY=your-tiktok-client-key
TIKTOK_CLIENT_SECRET=your-tiktok-client-secret

# Instagram/Facebook OAuth
INSTAGRAM_APP_ID=your-instagram-app-id
INSTAGRAM_APP_SECRET=your-instagram-app-secret

# Twitter/X OAuth
TWITTER_CLIENT_ID=your-twitter-client-id
TWITTER_CLIENT_SECRET=your-twitter-client-secret

# Optional: Analytics
GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
