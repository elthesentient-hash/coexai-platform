{
  "version": 2,
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "framework": null,
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/landing-page/index.html" },
    { "source": "/demo", "destination": "/demo/index.html" },
    { "source": "/dashboard", "destination": "/dashboard/index.html" }
  ],
  "routes": [
    { "src": "/demo", "dest": "/demo/index.html" },
    { "src": "/demo/(.*)", "dest": "/demo/index.html" },
    { "src": "/auth", "dest": "/auth/index.html" },
    { "src": "/auth/(.*)", "dest": "/auth/index.html" },
    { "src": "/dashboard", "dest": "/dashboard/index.html" },
    { "src": "/dashboard/(.*)", "dest": "/dashboard/index.html" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/landing-page/(.*)", "dest": "/landing-page/$1" }
  ],
  "headers": [
    {
      "source": "/landing-page/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    }
  ]
}
